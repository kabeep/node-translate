{
    "name": "@kabeep/node-translate",
    "version": "1.1.6",
    "type": "module",
    "description": "A powerful, secure and feature-rich api via Google Translation.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "docs": "jsdoc -c jsdoc.config.json",
        "lint": "xo src/**/*.ts --fix",
        "pretest": "npm run lint",
        "test": "vitest run",
        "coverage": "vitest run --coverage",
        "watch": "tsup --watch",
        "watch:test": "vitest",
        "build": "tsup --dts --treeshake smallest",
        "prepare": "husky"
    },
    "devDependencies": {
        "@types/lodash.has": "^4.5.9",
        "@types/node": "^22.2.0",
        "@vitest/coverage-istanbul": "^1.5.1",
        "docdash": "^2.0.2",
        "husky": "^9.0.11",
        "jsdoc": "^4.0.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5",
        "vitest": "^2.0.5",
        "xo": "^0.58.0"
    },
    "dependencies": {
        "got": "^13.0.0",
        "iso-639-1": "^3.1.2",
        "lodash.has": "^4.5.2",
        "os-locale": "^6.0.2"
    },
    "author": "Zhang Zixin (kabeep)",
    "homepage": "https://github.com/kabeep/node-translate#readme",
    "repository": "git@github.com:kabeep/node-translate.git",
    "bugs": "https://github.com/kabeep/node-translate/issue",
    "keywords": [
        "nodejs",
        "translate",
        "translate api",
        "google translate",
        "google translate api",
        "node translate",
        "node translate api"
    ],
    "engines": {
        "node": ">=16"
    },
    "license": "MIT"
}
